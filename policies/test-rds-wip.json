{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "Null": {
                    "aws:RequestTag/Name": "true",
                    "aws:RequestTag/dud": "true",
                    "aws:RequestTag/cloudservice": "true",
                    "aws:RequestTag/owner": "true",
                    "aws:RequestTag/createdby": "true",
                    "aws:RequestTag/accenture_ru": "true",
                    "aws:RequestTag/environment": "true",
                    "aws:RequestTag/location": "true",
                    "aws:RequestTag/project": "true"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "dud"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "owner"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "cloudservice"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "createdby"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "accenture_ru"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "environment"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "location"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "project"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": "rds:CreateDBInstance",
            "Resource": "arn:aws:rds:*:*:db:*",
            "Condition": {
                "ForAllValues:StringNotLike": {
                    "aws:TagKeys": "Name"
                },
                "StringNotEquals": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLike": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac"
                }
            }
        },
        {
            "Sid": "DenyNonGamiEc2Tag",
            "Effect": "Deny",
            "Action": [
                "ec2:RunInstances"
            ],
            "Resource": [
                "arn:aws:ec2:*:*:instance/*"
            ],
            "Condition": {
                "StringEqualsIfExists": {
                    "aws:RequestTag/nonGamiEc2": [
                        "deny"
                    ]
                },
                "Null": {
                    "aws:RequestTag/nonGamiEc2": "false"
                }
            }
        }
    ]
}