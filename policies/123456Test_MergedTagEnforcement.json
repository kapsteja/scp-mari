                "Null": {
                    "aws:RequestTag/environment": "true"
                },
                "StringNotEqualsIfExists": {
                    "aws:UserAgent": "application-autoscaling.amazonaws.com"
                },
                "ArnNotLikeIfExists": {
                    "aws:PrincipalArn": "arn:aws:iam::*:role/mi-iaac*"
                }
            }
